package inheritance;

public class Main2 {
	public static void main(String[] args) {
		Salaryman sm = new Salaryman();
		sm.name = "손흥민";
		sm.age = 30;
		sm.salary = 500;
		sm.getSalaryInfo();
		Student s = new Student();
		s.name = "소능민";
		s.age = 20;
		s.major = "컴공";
		s.getStudentInfo();
	}
}//end class
