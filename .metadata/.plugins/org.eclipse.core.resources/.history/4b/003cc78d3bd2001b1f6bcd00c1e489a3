package structure;

public class ClassName {
	public static void main(String[] args) {
		//Person 2명을 만들자
		PersonClass ps1 = new PersonClass();
		PersonClass ps2 = new PersonClass();
		
		//Person 2명 정보를 직접 기입
		ps1.name = "김영훈";
		ps1.age = 30;
		ps1.pNum = "010-8971-2838";
		ps1.address = "서울";
		ps1.sex = "M";
		ps1.glasses = true;
		
		ps1.name = "손흥민";
		ps1.age = 30;
		ps1.pNum = "010-1234-5678";
		ps1.address = "런던";
		ps1.sex = "M";
		ps1.glasses = false;
		
	}
}
