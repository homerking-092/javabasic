package heapmemory;

public class StringComparison {
	public static void main(String[] args) {
		String str1 = "Hello.";
		String str2 = "Hello";
		
		System.out.println(str1);
		System.out.println(str2);
		str1 = str2;
		System.out.println(str1);
		
		//자바에서는 문자열의 일부분만 변경할 수 없다
//		str1[i] = "a";
	}
}
