package interaction.game;

public class Archer {
	private int hp;
	private int atk;
	private int sld;
	private int mp;
	private int lv;
	private int exp;
	
	public Archer() {
		hp = 100;
		atk = 100;
		sld = 100;
		mp = 100;
		lv = 10;
		exp = 0;
	}
	
	public void HuntNashor(Nashor nashor) {
		hp -= 50;
		exp += 100;
		
	}
	public void HuntDragon() {
		
	}
	public void huntCrap() {
		
	}
}
