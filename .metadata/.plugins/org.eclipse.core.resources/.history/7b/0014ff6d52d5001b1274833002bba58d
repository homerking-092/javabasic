package interaction.game;

public class Nashor {
	private int hp;
	private int atk;
	private int sld;
	
	public Nashor(int hp, int sld, int atk) {
		this.hp = 30;
		this.atk = 30;
		this.sld = 30;
	}
	public int getNhp() {
		return hp;
	}
	
	public void NaInfo() {
		System.out.println("바론 스탯");
		System.out.println("hp: " + hp);
		System.out.println("akt: " + atk);
		System.out.println("sld: " + sld);
		System.out.println();
	}
	
	
	public void Atttack() {
		this.hp -= hp;
	}
	
	
	
	
}
