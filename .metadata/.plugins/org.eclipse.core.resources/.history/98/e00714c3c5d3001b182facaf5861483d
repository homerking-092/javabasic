package accessmodifier3;

import accessmodifier2.Child;
import accessmodifier2.Parent;

public class Main1 extends Parent {
	public static void main(String[] args) {
		//패키지가 다른(accesscodifier2의) child를 생성
		//패키지가 다르면 import까지 해줘야 쓸 수 있음
		Child child = new Child();
		
		//1.public 접근제한자
		System.out.println(child.a);
		//2.protected 접근제한자
		System.out.println(child.b);
		//1.private 접근제한자
		System.out.println(child.c);
		//1.default 접근제한자
		System.out.println(child.d);
	}
}
