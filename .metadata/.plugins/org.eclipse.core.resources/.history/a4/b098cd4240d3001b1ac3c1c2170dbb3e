package game;

public class Commoner {
	private int level;	//레벨
	private int hp;		//hp
	private int mp;		//mp
	private int sd;		//방어력
	private int exp;	//경험치
	private String id;	//id
	
	public Commoner(String id) {
		this.level = 1;
		this.hp = 100;
		this.mp = 100;
		this.sd = 10;
		this.exp = 0;
		this.id = id;
		
	}
		public void getInfo() {
			
		}
		
	
	
}
