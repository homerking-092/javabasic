package polymorphism;

public class Lingard extends Person {
	private int height;
	private String address;
	
	public Lingard(String name, int age, String address) {
		super(name, age);
		this.address = address;
	}
	
	public void showPerson() {
		super.showPerson();
		System.out.printf("소속팀: %d\n", address);
	}
}
