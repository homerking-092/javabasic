package interaction.game;

public class Warrior {
	private int hp;
	private int atk;
	private int sld;
	private int mp;
	private int lv;
	private int exp;
	
	public Warrior(int hp, int atk, int sld, int mp, int lv, int exp) {
		this.hp = hp;
		this.atk = atk;
		this.sld = sld;
		this.mp = mp;
		this.lv = lv;
		this.exp = exp;
	}
}
